{
  "name": "rest2ts_repo",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo test",
    "test:watch": "turbo run test:watch",
    "tsc": "turbo run tsc",
    "update-turborepo": "pnpm dlx @turbo/codemod@latest update"
  },
  "devDependencies": {
    "@rest2ts/eslint-config": "workspace:*",
    "@rest2ts/typescript-config": "workspace:*",
    "prettier": "^3.3.2",
    "turbo": "2.4.4"
  },
  "packageManager": "pnpm@9.15.5",
  "engines": {
    "node": ">=18"
  }
}
